<UserControl x:Class="BulbPicker.App.Views.DummyCardControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BulbPicker.App.Views"
             mc:Ignorable="d">
    <Border Style="{StaticResource CardStyle}" >
        <StackPanel>
            <TextBlock Text="Common Name" Style="{StaticResource CardTitleStyle}"/>
            <Border Style="{StaticResource CardSeparatorStyle}"/>
        </StackPanel>
    </Border>
</UserControl>
